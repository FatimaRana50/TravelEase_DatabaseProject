              <Mapping SourceColumn="Revenue" DataSetColumn="Revenue" />
              <Mapping SourceColumn="BookingMonth" DataSetColumn="BookingMonth" />
              <Mapping SourceColumn="AvgBookingValue" DataSetColumn="AvgBookingValue" />
            </Mappings>
